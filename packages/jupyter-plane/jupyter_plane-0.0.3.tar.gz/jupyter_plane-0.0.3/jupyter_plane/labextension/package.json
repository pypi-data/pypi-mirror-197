{
  "name": "@datalayer/jupyter-plane",
  "version": "0.0.3",
  "description": "Jupyter Plane.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/datalayer/jupyter-plane",
